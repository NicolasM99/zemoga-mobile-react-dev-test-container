{
  "module": "commonjs",
  "exclude": ["node_modules", "build"],
  "include": ["src", "**/*.tsx", "**/*.ts", "**/*.js"],
  "extends": "expo/tsconfig.base",
  "compileOnSave": true,
  "compilerOptions": {
    "noUnusedLocals": false,
    "importHelpers": true,
    "strict": true,
    "paths": {
      "src/*": ["src/*"]
    },
    "baseUrl": ".",
    "types": ["node", "jest", "@testing-library/jest-dom"]
  }
}
